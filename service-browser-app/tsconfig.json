{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "module": "ESNext",
        "jsx": "react-jsx",
        "moduleResolution": "Bundler",
        "allowImportingTsExtensions": true,
        "strict": true,
        "noEmit": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": [
        "src"
    ]
}
